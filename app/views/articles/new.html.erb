
<h1>Create a new article</h1>

<%= simple_form_for @article do |f|%>
<%= f.input :title %>
<%= f.input :content %>
<%= f.input :category %>
<%= f.simple_fields_for :resources do |resource| %>
  <%= resource.input :title %>
  <%= resource.input :link %>
  <% end %>
<%= f.submit "submit" %>
<% end %>
